    "description": "Laravel 携程 apollo 客户端",
    "authors": [
        {
            "name": "杨益民",
            "email": "744541711@qq.com"
        }
    ],
    "require": {
        "yiche/json-to-arr-str":"2.0.0",
        "yiche/simple-request": "dev-master",
        "yiche/unify-redis": "dev-master",
        "yiche/character-util": "dev-master",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
            "ApolloConfig\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ApolloConfig\\ServiceProvider"
            ]
        }
    },
    "repositories": {
        "3": {
            "type": "path",
            "url": "/home/wwwroot/personal-repository/simple-request",
            "options": {
                "symlink": true
            }
        },
        "4": {
            "type": "path",
            "url": "/home/wwwroot/personal-repository/unify-redis",
            "options": {
                "symlink": true
            }
        },
        "5": {
            "type": "path",
            "url": "/home/wwwroot/personal-repository/unvisible-character-filter",
            "options": {
                "symlink": true
            }
        }
        
    },
    "minimum-stability":"dev"
}
